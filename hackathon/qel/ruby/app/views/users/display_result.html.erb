            <div class="container">
             <div class="row">
                 <div class="col-md-12 main-title">
                 <h1>Report Analysis</h1>
                <!-- <p>Help us to improve our service and customer satisfaction.</p> -->
                </div>
       		</div>
            </div>

<section class="container" id="main">

<!-- Start Survey container -->
<div id="survey_container">

	
	<form name="example-1" id="wrapped" action="" method="POST" >
		<div id="middle-wizard">
			<div class="step">
				<div class="row">
					
                   
					<div class="col-md-6">
              <h3>Heamogram  </h3>        
						<ul class="data-list floated clearfix">
							
							<li><input name="hemogram" <%= params[:hemogram] == 'healthy' ? "checked=''" : nil%> type="radio" class="required check_radio" value="Male" ><label class="label_gender" > Healthy</label></li>
							<li><input name="hemogram" <%= params[:hemogram] == 'unhealthy' ? "checked=''" : nil%>  type="radio" class="required check_radio" value="Female"><label class="label_gender"> UnHealthy</label></li>
						</ul><br>

						<h3>Bio Chemical Parameters</h3> 
						<ul class="data-list floated clearfix">
							
							<li><input name="bio" <%= params[:bio] == 'healthy' ? "checked=''" : nil%>   type="radio" class="required check_radio" value="Male"><label class="label_gender"> Healthy</label></li>
							<li><input name="bio" <%= params[:bio] == 'unhealthy' ? "checked=''" : nil%>   type="radio" class="required check_radio" value="Female"><label class="label_gender"> UnHealthy</label></li>
						</ul>
                        
					</div><!-- end col-md-6 -->

					<div class="col-md-6">
              <h3>Lipid Test  </h3>        
						<ul class="data-list floated clearfix">
							
							<li><input name="lipid" <%= params[:lipid] == 'healthy' ? "checked=''" : nil%>    type="radio" class="required check_radio" value="Male"><label class="label_gender"> Healthy</label></li>
							<li><input name="lipid" type="radio" <%= params[:lipid] == 'unhealthy' ? "checked=''" : nil%>    class="required check_radio" value="Female"><label class="label_gender"> UnHealthy</label></li>
						</ul><br>

						<h3>Liver Function Test</h3> 
						<ul class="data-list floated clearfix">
							
							<li><input name="liver" <%= params[:liver] == 'healthy' ? "checked=''" : nil%>    type="radio" class="required check_radio" value="Male"><label class="label_gender"> Healthy</label></li>
							<li><input name="liver" <%= params[:liver] == 'unhealthy' ? "checked=''" : nil%>    type="radio" class="required check_radio" value="Female"><label class="label_gender"> UnHealthy</label></li>
						</ul>
                        
					</div><!-- end col-md-6 -->
				</div><!-- end row -->
                
				<div class="row">
					<div class="col-md-4 col-md-offset-4">
						
					</div>
				</div>
                
			</div><!-- end step-->
            
			
            
			
            
			
            
			<div class="submit step" id="complete">
                    	<i class="icon-check"></i>
						<h3>Survey complete! Thank you for you time.</h3>
						<button type="submit" name="process" class="submit">Submit the survey</button>
			</div><!-- end submit step -->
            
		</div><!-- end middle-wizard -->
        
		
	</form>
    
</div><!-- end Survey container -->

<div class="divider"></div>

<div class="row">
	<div class="col-md-12">
		<h3>Food Recommendations<span></span></h3>
	</div>
</div><!-- end row -->
<div style="text-align: center">
<div id="hemogram" style="<%= params[:hemogram] == 'healthy' ? 'display:none' : nil %>">
	<div class="row">
	<div class="col-md-6">
		<h4>Two egg per day </h4>
		<h4>100gm of beaf</h4>
		<h4>300gm of whole grain cereals</h4>
	</div>
	<div class="col-md-6">
		<h4>Dried Fruits & fruits </h4>
		<h4>Brocolli and Spinach</h4>
		<h4>Avoid Tea, Coffee, Wheat</h4>
	</div>
	</div>
</div>

<div id="bio_chemical" style="<%= params[:bio] == 'healthy' ? 'display:none' : nil %>">
	<div class="row">
	<div class="col-md-6">
		<h4>100gm of Berries</h4>
		<h4>Daily intake of cucumber and carrot is advisable</h4>
		<h4>250gm of strawberries and mushrooms</h4>
	</div>
	<div class="col-md-6">
		<h4>Drink plenty of water</h4>
		<h4>Avoid cereals</h4>
		<h4>Avoid aeriated drinks</h4>
	</div>
	</div>
</div>

<div id="lipid" style="<%= params[:lipid] == 'healthy' ? 'display:none' : nil %>">
	<div class="row">
	<div class="col-md-6">
		<h4>Intake of olive oil and red wine is advisable</h4>
		<h4>100gm of nuts, avocado and yogurt</h4>
	</div>
	<div class="col-md-6">
		<h4>150gm of whole grains</h4>
		<h4>Daily intake of fatty fish is advisable</h4>
	</div>
	</div>
</div>

<div id="liver" style="<%= params[:liver] == 'healthy' ? 'display:none' : nil %>">
	<div class="row">
	<div class="col-md-6">
		<h4>100gm of whole grains ,beans </h4>
		<h4>Daily intake of fish and walnut is advisable</h4>
	</div>
	<div class="col-md-6">
		<h4>Avoid alcohol</h4>
		<h4>Intake of chicken and soy is advisable</h4>
	</div>
	</div>
</div>

</div>
</section><!-- end section main container -->
       

 

