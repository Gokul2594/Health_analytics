    <script src="https://s3.ap-south-1.amazonaws.com/wp-myy-bucket-raju/assets/lib/tether/js/tether.min.js"></script>

    <script src="https://s3.ap-south-1.amazonaws.com/wp-myy-bucket-raju/assets/lib/perfect-scrollbar/js/perfect-scrollbar.jquery.min.js"></script>

    <script src="https://s3.ap-south-1.amazonaws.com/wp-myy-bucket-raju/assets/lib/bootstrap/dist/js/bootstrap.min.js"></script>


    <script src="https://s3.ap-south-1.amazonaws.com/wp-myy-bucket-raju/assets/js/app.js"></script>
    <script type="text/javascript">
      $(document).ready(function(){
      	//initialize the javascript
      	App.init();
      });
      
    </script>
   
  </body>
